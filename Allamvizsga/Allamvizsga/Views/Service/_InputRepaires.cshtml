<div class="form-group row">
    <div class="col-sm-3">Others:</div>
    <div class="col-sm-7">

        <textarea class="form-control" rows="5" data-bind="value: repaire.others"></textarea>

    </div>
</div>
<div class="form-group row">
    <div class="col-sm-3">Curent km:</div>
    <div class="col-sm-7">


        <input type="text" class="form-control" data-bind="value: repaire.currentKm">


    </div>
</div>
<div class="form-group row">
    <div class="col-sm-3">Next visit km:</div>
    <div class="col-sm-7">

        <input type="text" class="form-control" data-bind="value: repaire.nextVisitKm">

    </div>
</div>
<div class="form-group row">
    <div class="col-sm-3">Next visit Date</div>
    <div class="col-sm-7">

        <input type="date" class="form-control" data-bind="value: repaire.nextServiceDate">
    </div>
</div>

